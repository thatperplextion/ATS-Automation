    {
      "parameters": {
        "functionCode": "const resume = $input.first().json.resume_text||''; const jd = $input.first().json.job_description||''; const githubLangs = $input.first().json.github_languages||[];\nconst resumeWords = resume.toLowerCase().split(/\\W+/).filter(w=>w);\nconst jdWords = jd.toLowerCase().split(/\\W+/).filter(w=>w);\nconst requiredSkills = [...new Set(jdWords.filter(w=>w.length>2))].slice(0,100);\nconst current = [...new Set(resumeWords)].filter(w=>w.length>2).slice(0,200);\nconst skillsGap = requiredSkills.filter(s=>!current.includes(s));\nconst recommended = skillsGap.slice(0,10);\nreturn {json:{current_skills: current.slice(0,50), required_skills: requiredSkills.slice(0,50), skills_gap: skillsGap.slice(0,50), recommended_skills: recommended, certifications: [], courses: [], timeline:{months:3,plan:'Start with top recommended skills, build 2 projects, apply to relevant roles'}, career_roadmap:['Month 1: Learn fundamentals','Month 2: Build project','Month 3: Apply & network']}};"
      },
