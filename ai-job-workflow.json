      "parameters": {
        "functionCode": "const profile = $input[0].json || {};\nconst repos = Array.isArray($input[1].json) ? $input[1].json : ($input[1].json && $input[1].json[0] ? $input[1].json : []);\nconst languages = {};\nconst top = [];\nif(Array.isArray(repos)){\n  repos.forEach(r=>{ if(r.language) languages[r.language]=(languages[r.language]||0)+1; top.push({name:r.name, stars:r.stargazers_count||0, language:r.language||''});});\n}\nreturn {json:{github_username: profile.login||'', github_repos: profile.public_repos||0, github_followers: profile.followers||0, github_languages: Object.keys(languages), top_repos: top.slice(0,5), github_profile_url: profile.html_url||''}};"
