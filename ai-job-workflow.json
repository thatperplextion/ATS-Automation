    {
      "parameters": {
        "functionCode": "const resume = $input.first().json.resume_text||''; const jd = $input.first().json.job_description||''; const resumeWords = resume.toLowerCase().split(/\\W+/).filter(w=>w); const jdWords = jd.toLowerCase().split(/\\W+/).filter(w=>w);\nconst jdFreq = {};\njdWords.forEach(w=>{jdFreq[w]=(jdFreq[w]||0)+1;});\nconst matched=[]; const missing=[];\nconst uniqueJd = [...new Set(jdWords)];\nuniqueJd.forEach(w=>{ if(w && resumeWords.includes(w)) matched.push(w); else if(w) missing.push(w); });\n// simple scoring\nconst keywordScore = Math.round((matched.length/(uniqueJd.length||1))*100);\nconst formatScore = 80; // heuristic\nconst overall = Math.round((keywordScore*0.6 + formatScore*0.4));\nreturn {json:{ats_score: overall, keyword_match_score: keywordScore, format_quality_score: formatScore, overall_quality_score: overall, matched_keywords: matched.slice(0,50), missing_keywords: missing.slice(0,50), improvement_suggestions: ['Add missing keywords into skills/experience','Quantify achievements','Use clear section headings','Keep resume length under 2 pages'], section_feedback:{summary:'Add a concise 2-line summary',experience:'Use bullet points with metrics'}}};"
      },
      "id": "6ecd7780-5328-4ed9-86a1-b734605ab72d",
      "type": "n8n-nodes-base.function",
      "name": "Rule-based ATS & Keyword Matcher",
