    {
      "parameters": {
        "functionCode": "const resume = $input.first().json.resume_text||''; const suggestions = [];\n// simple heuristics: find lines with 'responsible' and suggest action verbs\nconst lines = resume.split(/\\n/).slice(0,200);\nlines.forEach(l=>{ if(/responsible|responsibilities|worked on/i.test(l)) suggestions.push('Replace passive wording with action verbs and quantify results'); });\nif(suggestions.length===0) suggestions.push('Highlight measurable achievements and technologies used.');\nreturn {json:{suggestions:suggestions.slice(0,10), suggested_professional_summary:'Experienced professional with practical experience in relevant technologies. Tailor this summary to the job description by including core keywords.'}};"
      },
